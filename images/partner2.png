  <ellipse cx="55" cy="130" rx="8" ry="18" fill="#228B22" transform="rotate(-30 55 130)"/>
  <ellipse cx="145" cy="70" rx="8" ry="18" fill="#228B22" transform="rotate(30 145 70)"/>

  <!-- Text -->
  <text x="100" y="165" font-family="Georgia, serif" font-size="16" fill="#C71585" text-anchor="middle" font-weight="bold">Wildflower</text>
  <text x="100" y="182" font-family="Georgia, serif" font-size="14" fill="#FF69B4" text-anchor="middle">Designs</text>
