<svg width="200" height="200" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="grad2" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#FF69B4;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#C71585;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background Circle -->
  <circle cx="100" cy="100" r="95" fill="#FFF5F7"/>

  <!-- Flower Design -->
  <!-- Center -->
  <circle cx="100" cy="100" r="10" fill="#FFD700"/>

  <!-- Petals -->
  <ellipse cx="100" cy="70" rx="15" ry="25" fill="url(#grad2)" opacity="0.9"/>
  <ellipse cx="130" cy="100" rx="25" ry="15" fill="url(#grad2)" opacity="0.9"/>
  <ellipse cx="100" cy="130" rx="15" ry="25" fill="url(#grad2)" opacity="0.9"/>
  <ellipse cx="70" cy="100" rx="25" ry="15" fill="url(#grad2)" opacity="0.9"/>

  <!-- Diagonal petals -->
  <ellipse cx="80" cy="80" rx="18" ry="22" fill="#FFB6C1" opacity="0.8" transform="rotate(-45 80 80)"/>
  <ellipse cx="120" cy="80" rx="18" ry="22" fill="#FFB6C1" opacity="0.8" transform="rotate(45 120 80)"/>
  <ellipse cx="120" cy="120" rx="18" ry="22" fill="#FFB6C1" opacity="0.8" transform="rotate(-45 120 120)"/>
  <ellipse cx="80" cy="120" rx="18" ry="22" fill="#FFB6C1" opacity="0.8" transform="rotate(45 80 120)"/>

