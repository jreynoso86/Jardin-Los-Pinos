  <rect x="140" y="90" width="5" height="15" rx="2" fill="#8B4513"/>

  <!-- Mountain/Nature Scene in Lens Reflection -->
  <path d="M 90 105 L 95 98 L 100 102 L 105 95 L 110 105 Z" fill="#228B22" opacity="0.4"/>

  <!-- Text -->
  <text x="100" y="160" font-family="Georgia, serif" font-size="14" fill="#2C3E50" text-anchor="middle" font-weight="bold">Rustic Lens</text>
