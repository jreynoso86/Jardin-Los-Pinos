<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="tablecloth" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#F5F5DC;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#FFFAF0;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#F5F5DC;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="plate" cx="50%" cy="40%">
      <stop offset="0%" style="stop-color:#FFFFFF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E8E8E8;stop-opacity:1" />
    </radialGradient>
  </defs>

  <!-- Background - warm amber lighting -->
  <rect width="800" height="600" fill="#F4E4C1"/>

  <!-- Wooden Floor -->
  <rect y="500" width="800" height="100" fill="#8B7355"/>
  <line x1="0" y1="510" x2="800" y2="510" stroke="#654321" stroke-width="2"/>
  <line x1="0" y1="530" x2="800" y2="530" stroke="#654321" stroke-width="2"/>
  <line x1="0" y1="550" x2="800" y2="550" stroke="#654321" stroke-width="2"/>
  <line x1="0" y1="570" x2="800" y2="570" stroke="#654321" stroke-width="2"/>

  <!-- Long Reception Tables -->
  <!-- Table 1 (left) -->
  <rect x="80" y="280" width="280" height="140" rx="5" fill="#8B4513"/>
  <rect x="80" y="300" width="280" height="100" fill="url(#tablecloth)"/>

  <!-- Table 2 (right) -->
  <rect x="440" y="280" width="280" height="140" rx="5" fill="#8B4513"/>
  <rect x="440" y="300" width="280" height="100" fill="url(#tablecloth)"/>

  <!-- Place Settings on Left Table -->
  <!-- Setting 1 -->
  <ellipse cx="130" cy="350" rx="25" ry="20" fill="url(#plate)"/>
  <circle cx="110" cy="340" r="8" fill="#E8E8E8"/>
  <rect x="145" y="340" width="3" height="25" fill="#C0C0C0"/>
  <rect x="150" y="340" width="3" height="25" fill="#C0C0C0"/>

  <!-- Setting 2 -->
  <ellipse cx="220" cy="350" rx="25" ry="20" fill="url(#plate)"/>
  <circle cx="200" cy="340" r="8" fill="#E8E8E8"/>
  <rect x="235" y="340" width="3" height="25" fill="#C0C0C0"/>
  <rect x="240" y="340" width="3" height="25" fill="#C0C0C0"/>

  <!-- Setting 3 -->
  <ellipse cx="310" cy="350" rx="25" ry="20" fill="url(#plate)"/>
  <circle cx="290" cy="340" r="8" fill="#E8E8E8"/>
  <rect x="325" y="340" width="3" height="25" fill="#C0C0C0"/>
  <rect x="330" y="340" width="3" height="25" fill="#C0C0C0"/>

  <!-- Place Settings on Right Table -->
  <!-- Setting 4 -->
  <ellipse cx="490" cy="350" rx="25" ry="20" fill="url(#plate)"/>
  <circle cx="470" cy="340" r="8" fill="#E8E8E8"/>
  <rect x="505" y="340" width="3" height="25" fill="#C0C0C0"/>
  <rect x="510" y="340" width="3" height="25" fill="#C0C0C0"/>

  <!-- Setting 5 -->
  <ellipse cx="580" cy="350" rx="25" ry="20" fill="url(#plate)"/>
  <circle cx="560" cy="340" r="8" fill="#E8E8E8"/>
  <rect x="595" y="340" width="3" height="25" fill="#C0C0C0"/>
  <rect x="600" y="340" width="3" height="25" fill="#C0C0C0"/>

  <!-- Setting 6 -->
  <ellipse cx="670" cy="350" rx="25" ry="20" fill="url(#plate)"/>
  <circle cx="650" cy="340" r="8" fill="#E8E8E8"/>
  <rect x="685" y="340" width="3" height="25" fill="#C0C0C0"/>
  <rect x="690" y="340" width="3" height="25" fill="#C0C0C0"/>

  <!-- Centerpieces - Mason Jars with Wildflowers -->
  <!-- Left Table -->
  <rect x="195" y="330" width="30" height="40" rx="3" fill="#B0C4DE" opacity="0.6"/>
  <line x1="210" y1="315" x2="207" y2="330" stroke="#228B22" stroke-width="2"/>
  <line x1="210" y1="315" x2="213" y2="330" stroke="#228B22" stroke-width="2"/>
  <circle cx="207" cy="312" r="4" fill="#FFB6C1"/>
  <circle cx="213" cy="310" r="4" fill="#FFD700"/>
  <circle cx="210" cy="315" r="4" fill="#FF69B4"/>

  <!-- Right Table -->
  <rect x="555" y="330" width="30" height="40" rx="3" fill="#B0C4DE" opacity="0.6"/>
  <line x1="570" y1="315" x2="567" y2="330" stroke="#228B22" stroke-width="2"/>
  <line x1="570" y1="315" x2="573" y2="330" stroke="#228B22" stroke-width="2"/>
  <circle cx="567" cy="312" r="4" fill="#9370DB"/>
  <circle cx="573" cy="310" r="4" fill="#FFFFFF"/>
  <circle cx="570" cy="315" r="4" fill="#FFA500"/>

