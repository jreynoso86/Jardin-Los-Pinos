  <line x1="0" y1="80" x2="800" y2="80" stroke="#DAA520" stroke-width="3"/>
  <circle cx="100" cy="80" r="8" fill="#FFD700" opacity="0.9"/>
  <circle cx="200" cy="80" r="8" fill="#FFD700" opacity="0.9"/>
  <circle cx="300" cy="80" r="8" fill="#FFD700" opacity="0.9"/>
  <circle cx="400" cy="80" r="8" fill="#FFD700" opacity="0.9"/>
  <circle cx="500" cy="80" r="8" fill="#FFD700" opacity="0.9"/>
  <circle cx="600" cy="80" r="8" fill="#FFD700" opacity="0.9"/>
  <circle cx="700" cy="80" r="8" fill="#FFD700" opacity="0.9"/>

  <line x1="0" y1="140" x2="800" y2="140" stroke="#DAA520" stroke-width="3"/>
  <circle cx="150" cy="140" r="8" fill="#FFD700" opacity="0.9"/>
  <circle cx="250" cy="140" r="8" fill="#FFD700" opacity="0.9"/>
  <circle cx="350" cy="140" r="8" fill="#FFD700" opacity="0.9"/>
  <circle cx="450" cy="140" r="8" fill="#FFD700" opacity="0.9"/>
  <circle cx="550" cy="140" r="8" fill="#FFD700" opacity="0.9"/>
  <circle cx="650" cy="140" r="8" fill="#FFD700" opacity="0.9"/>

  <!-- Wooden Beam -->
  <rect x="0" y="50" width="800" height="25" fill="#654321"/>

  <!-- Chairs -->
  <!-- Left side chairs -->
  <rect x="100" y="240" width="30" height="35" rx="3" fill="#8B4513"/>
  <rect x="190" y="240" width="30" height="35" rx="3" fill="#8B4513"/>
  <rect x="280" y="240" width="30" height="35" rx="3" fill="#8B4513"/>

  <rect x="100" y="425" width="30" height="35" rx="3" fill="#8B4513"/>
  <rect x="190" y="425" width="30" height="35" rx="3" fill="#8B4513"/>
  <rect x="280" y="425" width="30" height="35" rx="3" fill="#8B4513"/>

  <!-- Right side chairs -->
  <rect x="460" y="240" width="30" height="35" rx="3" fill="#8B4513"/>
  <rect x="550" y="240" width="30" height="35" rx="3" fill="#8B4513"/>
  <rect x="640" y="240" width="30" height="35" rx="3" fill="#8B4513"/>

  <rect x="460" y="425" width="30" height="35" rx="3" fill="#8B4513"/>
  <rect x="550" y="425" width="30" height="35" rx="3" fill="#8B4513"/>
  <rect x="640" y="425" width="30" height="35" rx="3" fill="#8B4513"/>

  <!-- Decorative Elements -->
  <!-- Hay bale corner decoration -->
  <rect x="20" y="450" width="40" height="35" rx="4" fill="#DAA520"/>
  <rect x="20" y="442" width="40" height="10" rx="2" fill="#B8860B"/>

  <rect x="740" y="450" width="40" height="35" rx="4" fill="#DAA520"/>
  <rect x="740" y="442" width="40" height="10" rx="2" fill="#B8860B"/>

  <!-- Text -->
  <text x="400" y="30" font-family="Arial, sans-serif" font-size="26" fill="#654321" text-anchor="middle" font-weight="bold">Elegant Reception Setup</text>
  <text x="400" y="560" font-family="Arial, sans-serif" font-size="16" fill="#333" text-anchor="middle">Farm-to-table dining experience</text>
</svg>