<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="sky" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#87CEEB;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#E0F6FF;stop-opacity:1" />
    </linearGradient>
    <linearGradient id="wood" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#8B4513;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#A0522D;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#8B4513;stop-opacity:1" />
    </linearGradient>
  </defs>

  <!-- Background Sky -->
  <rect width="800" height="600" fill="url(#sky)"/>

  <!-- Floor -->
  <rect y="400" width="800" height="200" fill="#654321"/>

  <!-- Barn Interior Walls -->
  <rect x="50" y="100" width="700" height="300" fill="#8B7355"/>

  <!-- Wooden Beams -->
  <rect x="0" y="80" width="800" height="30" fill="url(#wood)"/>
  <rect x="150" y="110" width="25" height="290" fill="url(#wood)"/>
  <rect x="350" y="110" width="25" height="290" fill="url(#wood)"/>
  <rect x="550" y="110" width="25" height="290" fill="url(#wood)"/>
  <rect x="650" y="110" width="25" height="290" fill="url(#wood)"/>

  <!-- Chandelier Lights -->
  <circle cx="200" cy="120" r="15" fill="#FFD700" opacity="0.8"/>
  <circle cx="400" cy="120" r="15" fill="#FFD700" opacity="0.8"/>
  <circle cx="600" cy="120" r="15" fill="#FFD700" opacity="0.8"/>

  <!-- Strings of lights -->
  <line x1="100" y1="140" x2="700" y2="140" stroke="#FFD700" stroke-width="2"/>
  <circle cx="150" cy="140" r="5" fill="#FFFF00" opacity="0.9"/>
  <circle cx="250" cy="140" r="5" fill="#FFFF00" opacity="0.9"/>
  <circle cx="350" cy="140" r="5" fill="#FFFF00" opacity="0.9"/>
  <circle cx="450" cy="140" r="5" fill="#FFFF00" opacity="0.9"/>
  <circle cx="550" cy="140" r="5" fill="#FFFF00" opacity="0.9"/>
  <circle cx="650" cy="140" r="5" fill="#FFFF00" opacity="0.9"/>

  <!-- Tables and Chairs Setup -->
  <!-- Round Table 1 -->
  <ellipse cx="250" cy="320" rx="60" ry="30" fill="#654321"/>
  <rect x="245" y="340" width="10" height="40" fill="#654321"/>

  <!-- Round Table 2 -->
  <ellipse cx="550" cy="320" rx="60" ry="30" fill="#654321"/>
  <rect x="545" y="340" width="10" height="40" fill="#654321"/>

  <!-- Chairs around tables -->
  <rect x="190" y="300" width="20" height="25" rx="3" fill="#8B4513"/>
  <rect x="310" y="300" width="20" height="25" rx="3" fill="#8B4513"/>
  <rect x="230" y="285" width="20" height="25" rx="3" fill="#8B4513"/>
  <rect x="270" y="340" width="20" height="25" rx="3" fill="#8B4513"/>

  <rect x="490" y="300" width="20" height="25" rx="3" fill="#8B4513"/>
  <rect x="610" y="300" width="20" height="25" rx="3" fill="#8B4513"/>
  <rect x="530" y="285" width="20" height="25" rx="3" fill="#8B4513"/>
  <rect x="570" y="340" width="20" height="25" rx="3" fill="#8B4513"/>

  <!-- Hay bales decoration -->
  <rect x="80" y="350" width="50" height="40" rx="5" fill="#DAA520"/>
  <rect x="80" y="340" width="50" height="15" rx="3" fill="#B8860B"/>
  <rect x="670" y="350" width="50" height="40" rx="5" fill="#DAA520"/>
  <rect x="670" y="340" width="50" height="15" rx="3" fill="#B8860B"/>

  <!-- Window -->
  <rect x="620" y="180" width="100" height="120" fill="#87CEEB" opacity="0.6"/>
  <rect x="620" y="180" width="100" height="4" fill="#654321"/>
  <rect x="668" y="180" width="4" height="120" fill="#654321"/>

  <!-- Text -->
  <text x="400" y="550" font-family="Arial, sans-serif" font-size="28" fill="#FFF" text-anchor="middle" font-weight="bold">Rustic Barn Interior</text>
  <text x="400" y="580" font-family="Arial, sans-serif" font-size="16" fill="#FFF" text-anchor="middle">Elegant indoor ceremony and reception space</text>
</svg>